# Google Gemini
GOOGLE_API_KEY = "your-Google-api-key"

# DB Connection
DB_URL = "postgresql+asyncpg://postgres.[username]:[password]@[host]:[port]/[database-name]" # in Supabase. Try using pooling connection string if you have issues with connection limits.

# For local development, you can use the following connection string to postgres container in docker-compose:
# DB_URL = postgresql+asyncpg://{DB_USER}:{DB_PASSWORD}@{DB_HOST}:{DB_PORT}/{DB_NAME}

# Postgres Database Configuration
POSTGRES_PORT = 5432
POSTGRES_USER =" your-user"
POSTGRES_PASSWORD = "your-password"
POSTGRES_DB = "your-database-name"
POSTGRES_HOST = "postgres"

# DOCKER
RESTART_POLICY = "unless-stopped"
DOCKER_VOLUME_DIRECTORY = "."

# Milvus Configuration
MILVUS_URI = "http://localhost:19530"

# Zilliz Configuration
ZILLIZ_URI = "your-zilliz_uri"
ZILLIZ_TOKEN = "your-zilliz_token"

# API Key VoyageAI
VOYAGE_API_KEY = "your-voyage-api-key"

# API Key Name
API_KEY_NAME = "mysecretapikey"

# Json Web Token
SECRET_KEY = "your-jwt"
ALGORITHM = "HS256"
ACCESS_TOKEN_EXPIRE_MINUTES = 60

# Langchain API conection
LANGCHAIN_URI = "http://localhost:80"
LANGCHAIN_API_KEY = "a-langchain-api-key"

# Google OAuth2 credentials
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"